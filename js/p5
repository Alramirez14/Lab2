function setup() {
  createCanvas(800,400);
}

function draw() {
  background(135, 206, 235);
  
  
  fill("rgb(208,208,140)");
  stroke("brown");
  rect(400, 100, 50, 200);
  rect(290, 220, 100, 80);
  rect(460, 150, 100, 150);
  rect(480, 200, 50, 110);
  triangle(290, 220, 340, 180, 390, 220);
  
  fill("rgb(0,200,255)")
  rect(405, 105, 10, 50);
  rect(420, 105, 10, 50)
  rect(435, 105, 10, 50)
  
  rect(300, 230, 30, 50)
  rect(350, 230, 30, 50)
  
  
  fill("green");
  stroke("green");
  rect(0, 300, 800, 20)
  quad(500, 300, 550, 200, 750, 250, 790, 300);
  quad(100, 300, 200, 260, 250, 250, 325, 300);

  fill("tan")
  rect(0, 320, 800, 20)
  
  fill("yellow")
  circle(250, 100, 100)
  
  textSize(75)
  text("🌊", 10, 400)
  text("🌊", 80, 400)
  text("🌊", 150, 400)
  text("🏄🏼", 10, 400)
  text("🦈", 650, 400)
  text("🐟", 570, 390)
  
}